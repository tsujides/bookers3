<% if user_signed_in? %>
    <%= form_tag(search_path, method: :get) do %>
      <%= text_field_tag('content', value = @content) %>
      <%= select_tag 'model', options_for_select({ "User" => :user, "Book" => :book }, :selected => @model) %>
      <%= select_tag 'type', options_for_select({ "完全一致" => :perfect, "部分一致" => :partial }, :selected => @type) %>
      <%= submit_tag '検索' %>
    <% end %>
<% end %>